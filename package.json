    "@types/leaflet": "^1.7.3",
    "@types/mapbox-gl": "^2.3.0",
    "@types/node": "^12.20.15",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
